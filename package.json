  },
  "description": "Admin Dashboard with Windowed components",
  "keywords": [
    "react",
    "windows",
    "dashboard",
    "admin",
    "windowed",
    "window"
  ],
  "dependencies": {
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-draggable": "^4.4.4",
    "react-resizable": "^3.0.4",
    "react-scripts": "^4.0.1",
    "reactizy": "^4.1.4",
    "webpack-cli": "^4.8.0"
  },
  "main": "./dist/index.js",
  "types": "/dist/main.d.ts",
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf ./dist; ./node_modules/webpack/bin/webpack.js && npx tsc",
    "doc": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
