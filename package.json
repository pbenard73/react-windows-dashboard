{
  "name": "react-windows-dashboard",
  "version": "0.2.0",
  "private": false,
  "license": "ISC",
  "homepage": "https://pbenard73.github.io/react-windows-dashboard",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbenard73/react-windows-dashboard"
  },
  "description": "Admin Dashboard with Windowed components",
  "keywords": [
    "react",
    "windows",
    "dashboard",
    "admin",
    "windowed",
    "window"
  ],
  "dependencies": {
    "react-draggable": "^4.4.4",
    "react-resizable": "^3.0.4"
  },
  "devDependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.9.1",
    "@types/react-resizable": "^3.0.3",
    "@types/styled-components": "^5.1.26",
    "array-move": "^4.0.0",
    "babel-plugin-styled-components": "^2.0.7",
    "clean-webpack-plugin": "^4.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^8.0.5",
    "react-respo": "^0.1.3",
    "react-scripts": "^5.0.1",
    "react-syntax-highlighter": "^15.4.4",
    "source-map-explorer": "^2.5.3",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.0",
    "webpack-cli": "^4.8.0"
  },
  "main": "./dist/index.js",
  "types": "/dist/main.d.ts",
  "scripts": {
    "start": "react-scripts start",
    "build": "npx tsc; rm -rf ./dist; ./node_modules/webpack/bin/webpack.js; cp build/*.d.ts dist/.; ",
    "doc": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'dist/*.js' --no-border-checks"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      ["babel-plugin-styled-components", {"pure":true}]
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
